		{
			"name": "GET lists",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Verify that the id is unique for all movies\", function(){\r",
							"    //extract the id from response.json\r",
							"    var response_json = pm.response.json().results;\r",
							"\r",
							"    //extract the values of id\r",
							"    var id_values = response_json.map(movie => movie.id);\r",
							"\r",
							"    //vizualise in console the id values\r",
							"    console.log(\"The id Values are:\", id_values);\r",
							"\r",
							"    //verify if there are duplicated values of id and remove them with set\r",
							"    var unique_id = new Set(id_values);\r",
							"\r",
							"    //compare the size of the original array with the size of the set\r",
							"    pm.expect(unique_id.size).to.equal(id_values.length);\r",
							"});\r",
							"\r",
							"pm.test(\"Verify that description field is not empty for all the movies\", function (){\r",
							"    //extract the description field from all movies\r",
							"    var response_json = pm.response.json().results;\r",
							"\r",
							"    //extract the characters from all the description fields\r",
							"    var description_field_values = response_json.map (movie => movie.description);\r",
							"\r",
							"    //vizualize in console the description field values\r",
							"    console.log(\"The description field values are: \", description_field_values);\r",
							"\r",
							"    //assertion that the description fields are not empty (Not all movies have description and an error will appear)\r",
							"    description_field_values.forEach(function(description, index){\r",
							"        pm.expect(description, \"Description field should not be empty for Movie \" + (index + 1)).to.not.be.empty;\r",
							"    })\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Verify that the list_type is 'movie' for all the movies\", function () {\r",
							"    listtype_is_movie = true\r",
							"\r",
							"\r",
							"}  ) \r",
							""
						],
